void initDac(int pinNr) {
  dacCS = pinNr;
  pinMode(pinNr, OUTPUT);
  SPI.begin();
}
void writeToDac(byte data) {
  digitalWrite(dacCS, LOW);
  byte upper = B00110000 | (data >> 4);
  byte lower = (data << 4) & 0xFF;
  SPI.transfer(upper);
  SPI.transfer(lower);
  digitalWrite(dacCS, HIGH);
}

void shutdownDac() {
  digitalWrite(dacCS, LOW);
  byte upper = B00100000;
  byte lower = 0;
  SPI.transfer(upper);
  SPI.transfer(lower);
  digitalWrite(dacCS, HIGH);
  SPI.end();
}

void setup() 
{
 initDac(0);
}

void loop() 
{
  startPlayback(samples, sizeof(samples));
  delay(3000);

}


